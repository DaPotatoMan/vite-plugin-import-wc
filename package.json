{
  "name": "vite-plugin-import-wc",
  "version": "1.0.0",
  "type": "module",
  "description": "Vite plugin for importing vue component as web component",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
