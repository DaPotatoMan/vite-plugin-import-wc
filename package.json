    "build": "tsup-node src/index.ts --clean --format=esm --dts"
  },
  "author": "dapotatoman",
  "license": "MIT",
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^8.55.0",
    "jsdom": "^23.0.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vitest": "^1.0.3",
    "vue": "^3.3.11"
  }
}